{
  "name": "app_directory_name",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:eslint": "next lint --dir src",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier . --debug-check",
    "lint:markup": "markuplint 'src/**/*.tsx'",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "fix": "run-s fix:*",
    "fix:eslint": "next lint --fix",
    "fix:prettier": "prettier . --write",
    "fix:style": "stylelint '**/*.{css,scss,sass}' --fix && prettier . --write",
    "package:check": "npx -p npm-check-updates -c 'ncu'",
    "package:update": "npx npm-check-updates -u",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "14.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
